<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" >

    <xacro:include filename="inertial_macros.xacro"/>


    <xacro:property name="chassis_length" value="0.5"/>
    <xacro:property name="chassis_width" value="0.5"/>
    <xacro:property name="chassis_height" value="0.2"/>
    <xacro:property name="chassis_mass" value="50.0"/>
    <xacro:property name="wheel_radius" value="0.08"/>
    <xacro:property name="wheel_thickness" value="0.03"/>
    <xacro:property name="wheel_mass" value="0.02"/>
    <xacro:property name="wheel_offset_x" value="0.5"/>
     <xacro:property name="wheel_offset_xy" value="0.248"/>
    <xacro:property name="wheel_offset_y" value="0.25227"/>
    <xacro:property name="wheel_offset_z" value="0.09"/>


    <xacro:property name="middle_wheel_offset_x" value="0.0" />
    <xacro:property name="middle_wheel_offset_y" value="0.25227" />


    <material name="white">
        <color rgba="1 1 1 1" />
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="red">
        <color rgba="1 0 0 1"/>
    </material>

    <material name="grey">
      <color rgba="0.5 0.5 0.5 1"/>
    </material>

    <!-- BASE LINK -->

    <link name="base_link">
      <self_collide>false</self_collide>
      <laser_retro>0.0</laser_retro>
    </link>

    <!-- BASE_FOOTPRINT LINK -->

    <joint name="base_footprint_joint" type="fixed">
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <origin xyz="0 0 0" rpy="0 0 0"/>
    </joint>

    <link name="base_footprint">
    </link>


    <!-- CHASSIS LINK -->

    <joint name="chassis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="0 0 0"/>
    </joint>

    <link name="chassis">
        <xacro:inertial_box mass="50" x="${chassis_length}" y="${chassis_width}" z="${chassis_height}">
            <origin xyz="0 0 0" rpy="0 0 0"/>
        </xacro:inertial_box>
        <visual>
        <origin
            xyz="0 0 0"
            rpy="0 0 0" />
        <geometry>
            <mesh
           filename="file://$(find ia_robot_urdf)/meshes/base_link.stl" />
        </geometry>
        <material
            name="">
            <color
            rgba="0.741176470588235 0.901960784313726 0.956862745098039 1" />
        </material>
        </visual>
        <collision>
        <origin
            xyz="0 0 0"
            rpy="0 0 0" />
        <geometry>
            <mesh
           filename="file://$(find ia_robot_urdf)/meshes/base_link.stl" />
        </geometry>
        </collision>
    </link>



    <!-- LEFT WHEEL LINK -->
  <link
    name="lf_wheel_link">
    <inertial>
      <origin
       xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="${wheel_mass}" />
      <inertia
        ixx="0.1"
        ixy="0"
        ixz="0"
        iyy="0.1"
        iyz="0"
        izz="0.000456492835033346" />
    </inertial>
    <visual>
      <origin
          xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
         filename="file://$(find ia_robot_urdf)/meshes/lf_wheel.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0 0 0 1" />
      </material>
    </visual>
<collision>
  <origin
    xyz="0 0 0"
    rpy="-1.5708 0 0" />
  <geometry>
    <cylinder
      radius="${wheel_radius}"
      length="${wheel_thickness}" />
  </geometry>
</collision>
  </link>
  <joint
    name="lf_wheel_joint"
     type="fixed">
    <origin
      xyz="${wheel_offset_x} ${wheel_offset_y} ${wheel_offset_z}"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="lf_wheel_link" />
    <axis
      xyz="0 1 0" />
  </joint>
  <link
    name="lb_wheel_link">
    <inertial>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="${wheel_mass}"  />
      <inertia
        ixx="0.1"
        ixy="0"
        ixz="0"
        iyy="0.1"
        iyz="0"
        izz="0.000456492835033346" />
    </inertial>
    <visual>
      <origin
        xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
         filename="file://$(find ia_robot_urdf)/meshes/lb_wheel.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0 0 0 1" />
      </material>
    </visual>
<collision>
  <origin
    xyz="0 0 0"
    rpy="-1.5708 0 0" />
  <geometry>
    <cylinder
      radius="${wheel_radius}"
      length="${wheel_thickness}" />
  </geometry>
</collision>
  </link>
  <joint
    name="lb_wheel_joint"
    type="fixed">
    <origin
      xyz="-${wheel_offset_x} ${wheel_offset_y} ${wheel_offset_z}"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="lb_wheel_link" />
    <axis
      xyz="0 1 0" />
  </joint>
  <link
    name="rb_wheel_link">
    <inertial>
      <origin
       xyz="0 0 0"
        rpy="0 0 0" />
      <mass
       value="${wheel_mass}"  />
         <inertia
        ixx="0.1"
        ixy="0"
        ixz="0"
        iyy="0.1"
        iyz="0"
        izz="0.000456492835033346" />
    </inertial>
    <visual>
      <origin
          xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
         filename="file://$(find ia_robot_urdf)/meshes/rb_wheel.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0 0 0 1" />
      </material>
    </visual>
<collision>
  <origin
    xyz="0 0 0"
    rpy="-1.5708 0 0" />
  <geometry>
    <cylinder
      radius="${wheel_radius}"
      length="${wheel_thickness}" />
  </geometry>
</collision>
  </link>
  <joint
    name="rb_wheel_joint"
    type="fixed">
    <origin
      xyz="-${wheel_offset_x} -${wheel_offset_y} ${wheel_offset_z}"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="rb_wheel_link" />
    <axis
      xyz="0 1 0" />
  </joint>
  <link
    name="rf_wheel_link">
    <inertial>
      <origin
       xyz="0 0 0"
        rpy="0 0 0" />
      <mass
        value="${wheel_mass}"  />
      <inertia
        ixx="0.1"
        ixy="0"
        ixz="0"
        iyy="0.1"
        iyz="0"
        izz="0.000456492835033346" />
    </inertial>
    <visual>
      <origin
         xyz="0 0 0"
        rpy="0 0 0" />
      <geometry>
        <mesh
         filename="file://$(find ia_robot_urdf)/meshes/rf_wheel.stl" />
      </geometry>
      <material
        name="">
        <color
          rgba="0 0 1 1" />
      </material>
    </visual>
<collision>
  <origin
    xyz="0 0 0"
    rpy="-1.5708 0 0" />
  <geometry>
    <cylinder
      radius="${wheel_radius}"
      length="${wheel_thickness}" />
  </geometry>
</collision>
  </link>
  <joint
    name="rf_wheel_joint"
    type="fixed">
    <origin
      xyz="${wheel_offset_x} -${wheel_offset_y} ${wheel_offset_z}"
      rpy="0 0 0" />
    <parent
      link="base_link" />
    <child
      link="rf_wheel_link" />
    <axis
      xyz="0 1 0" />
  </joint>


  <!-- Left Middle Wheel -->
<link name="lm_wheel_link">
  <inertial>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <mass value="${wheel_mass}" />
    <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.000456492835033346" />
  </inertial>
  <collision>
    <origin xyz="0 0 0" rpy="-1.5708 0 0" />
    <geometry>
      <cylinder radius="${wheel_radius}" length="${wheel_thickness}" />
    </geometry>
  </collision>
</link>

<joint name="lm_wheel_joint" type="continuous">
  <origin xyz="${middle_wheel_offset_x} ${middle_wheel_offset_y} ${wheel_offset_z}" rpy="0 0 0" />
  <parent link="base_link" />
  <child link="lm_wheel_link" />
  <axis xyz="0 1 0" />
</joint>

<!-- Right Middle Wheel -->
<link name="rm_wheel_link">
  <inertial>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <mass value="${wheel_mass}" />
    <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.000456492835033346" />
  </inertial>
  <collision>
    <origin xyz="0 0 0" rpy="-1.5708 0 0" />
    <geometry>
      <cylinder radius="${wheel_radius}" length="${wheel_thickness}" />
    </geometry>
  </collision>
</link>

<joint name="rm_wheel_joint" type="continuous">
  <origin xyz="${middle_wheel_offset_x} -${middle_wheel_offset_y} ${wheel_offset_z}" rpy="0 0 0" />
  <parent link="base_link" />
  <child link="rm_wheel_link" />
  <axis xyz="0 1 0" />
</joint>

</robot>